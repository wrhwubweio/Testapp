<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainPage">
    <fragment
        android:id="@+id/mainPage"
        android:name="com.example.testapp.MainPage"
        android:label="MainPage" >
        <action
            android:id="@+id/action_mainPage_to_testPage"
            app:destination="@id/testPage" />
        <action
            android:id="@+id/action_mainPage_to_profile2"
            app:destination="@id/profile" />
        <action
            android:id="@+id/action_mainPage_to_library22"
            app:destination="@id/library" />
    </fragment>
    <fragment
        android:id="@+id/testPage"
        android:name="com.example.testapp.TestPage"
        android:label="TestPage" >
        <action
            android:id="@+id/action_testPage_to_mainPage"
            app:destination="@id/mainPage" />
    </fragment>
    <fragment
        android:id="@+id/profile"
        android:name="com.example.testapp.Profile"
        android:label="Profile" >
        <action
            android:id="@+id/action_profile2_to_mainPage"
            app:destination="@id/mainPage" />
        <action
            android:id="@+id/action_profile2_to_library2"
            app:destination="@id/library" />
    </fragment>
    <fragment
        android:id="@+id/library"
        android:name="com.example.testapp.Library"
        android:label="Library" >
        <action
            android:id="@+id/action_library2_to_profile2"
            app:destination="@id/profile" />
        <action
            android:id="@+id/action_library2_to_mainPage"
            app:destination="@id/mainPage" />
    </fragment>
</navigation>